<div class="card col-lg-3 col-md-6 col-xs-12">
		  <%= image_tag search.image[0].url,class:"card-img-top img-md",alt:search.name) %>
		  <div class="card-body">
			<p class="card-text pre-header-text"><%= listing.sort.sort + " . " + search.city%></p>
			<p class="card-text header-text"><%= link_to search.name,airbnb_path(search)%></p>
			<p class="card-text price-text"><%= "RM " + search.price_per_night.to_s[0..-3] + " per night"%></p>
		  </div>
</div>